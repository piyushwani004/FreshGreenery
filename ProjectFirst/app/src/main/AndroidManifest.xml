<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.piyush004.projectfirst">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/food1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/food1"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.DayNight.NoActionBar.Bridge">
        <activity android:name=".owner.home.HomeOwnerActivity"></activity>
        <activity android:name=".Dashboard.CustomerDashboard" />
        <activity android:name=".Dashboard.OwnerDashboard" />
        <activity
            android:name=".Auth.SignUpActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Auth.LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>